<?xml version="1.0" encoding="utf-8"?>

<templates id="template" xml:space="preserve">
 <t t-name="OdooMap" >
   <label for="lat">خط العرض:</label>
  <input type="text" class="lat" id="lat" name="lat"></input>
  <label for="long">خط الطول:</label>
  <input type="text" id="long"  class="long" name="long"></input>
  <label for="meter">المدى المسموح به بالمتر:</label>
  <input type="text" id="meter"  class="meter" name="meter"></input>
  <br>
   <button type="button" class="submit" id="submit" name="submit">اعتمد الإحداثيات المختارة</button> 
   </br>
<br></br>
    <div widget="100%" height="100%" style="margin: 0;padding: 0;">
    <div id="map" style="height:500px;width:900px;"></div>
        <script src="https://maps.googleapis.com/maps/api/js?libraries=places&amp;sensor=false&amp;callback=initMap&amp;key=#[google_location_apikey]"
async="true"></script>
        </div>
    </t>
</templates>
